<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sanathbhardwaj.svpartner">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logopartner"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logopartner"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menu.ProfileActivity"></activity>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menu.Analytics" />
        <activity android:name=".menu.AllProducts" />
        <activity android:name=".menu.DisplayName" />
        <activity android:name=".menu.Credits" />
        <activity android:name=".menu.ChangeBanner" />
        <activity android:name=".menu.OrdersActivity" />
        <activity android:name=".menu.AddProduct" />
        <activity android:name=".CompleteForm" />
        <activity android:name=".GpsSelector" />
        <activity android:name=".CurrentLocation" />
        <activity android:name=".PhoneEntry" />
        <activity android:name=".PhoneAuth" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>